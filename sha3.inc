ifndef SHA3_H
SHA3_H	EQU	<>

SHA3_ROUNDS	      EQU	24
SHA3_STATE_LEN	  EQU	25

SHA3_224	        EQU	0
SHA3_224_HASH_LEN	EQU	28
SHA3_224_BLK_LEN	EQU	144

SHA3_256	        EQU	1
SHA3_256_HASH_LEN	EQU	32
SHA3_256_BLK_LEN	EQU	136

SHA3_384	        EQU	2
SHA3_384_HASH_LEN	EQU	48
SHA3_384_BLK_LEN	EQU	104

SHA3_512	        EQU	3
SHA3_512_HASH_LEN	EQU	64
SHA3_512_BLK_LEN	EQU	72

SHA3_CTX	struct 
  state	  dq SHA3_STATE_LEN dup (?)
  index	  dd	?
  dgstlen	dd	?
  rounds	dd	?
  blklen	dd	?
  blk	    db 256 dup (?)
SHA3_CTX	ends

endif